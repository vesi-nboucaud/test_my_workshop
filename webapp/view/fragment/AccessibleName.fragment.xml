<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:uxap="sap.uxap"
>
    <uxap:ObjectPageSection
        id="idAccessibleNameObjectPageSection"
        title="{i18n>AccessibleName.fragment.title}"
        titleLevel="H2"
        titleUppercase="false"
        showTitle="false"
        class="sapUiSmallMarginTop"
    >
        <uxap:subSections>
            <core:Fragment
                id="idProvidingLabelsForInputControlsFragment"
                fragmentName="com.vesi.zfaca_accessibility_training.view.fragment.AccessibleName.ProvidingLabelsForInputControls"
                type="XML"
            />
            <core:Fragment
                id="idUsingInvisibleTextForGroupedInputControlsFragment"
                fragmentName="com.vesi.zfaca_accessibility_training.view.fragment.AccessibleName.UsingInvisibleTextForGroupedInputControls"
                type="XML"
            />
            <core:Fragment
                id="idInputDescriptionManagedByAriaDescribedByFragment"
                fragmentName="com.vesi.zfaca_accessibility_training.view.fragment.AccessibleName.InputDescriptionManagedByAriaDescribedBy"
                type="XML"
            />
            <core:Fragment
                id="idProvidingTitlesForTablesFragment"
                fragmentName="com.vesi.zfaca_accessibility_training.view.fragment.AccessibleName.ProvidingTitlesForTables"
                type="XML"
            />
            <core:Fragment
