                id="idProvidingTooltipsForIconTabsFragment"
                fragmentName="com.vesi.zfaca_accessibility_training.view.fragment.AccessibleName.ProvidingTooltipsForIconTabs"
                type="XML"
            />
            <core:Fragment
                id="idProvidingLabelsForPopupsFragment"
                fragmentName="com.vesi.zfaca_accessibility_training.view.fragment.AccessibleName.ProvidingLabelsForPopups"
                type="XML"
            />
